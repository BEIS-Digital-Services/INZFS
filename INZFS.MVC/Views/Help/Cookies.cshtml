@{
    ViewData["Title"] = "Cookie Policy";
}

    <div class="govuk-grid-row">
        <div class="govuk-grid-column-three-quarters">
            <nav aria-label="breadcrumb" class="govuk-breadcrumbs">
                <ol class="govuk-breadcrumbs__list">
                    <li class="govuk-breadcrumbs__list-item">
                        <a class="govuk-breadcrumbs__link" href="/">
                            Apply for the Energy Entrepreneurs Fund: phase 9
                        </a>
                    </li>
                    <li class="govuk-breadcrumbs__list-item" aria-current="page">
                        Support
                    </li>
                </ol>
            </nav>
        </div>
        <div class="govuk-grid-column-one-quarter govuk-youraccount-right"></div>
    </div>

    <div class="cookie-settings__confirmation" data-cookie-confirmation="true">

        <div class="gem-c-success-alert govuk-notification-banner govuk-notification-banner--success" role="alert" tabindex="-1" aria-labelledby="govuk-notification-banner-title-4f62ca90" data-module="initial-focus">
            <div class="govuk-notification-banner__header">
                <h2 class="govuk-notification-banner__title" id="govuk-notification-banner-title-4f62ca90">Success</h2>
            </div>
            <div class="govuk-notification-banner__content">
                <h3 class="govuk-notification-banner__heading">Your cookie settings were saved</h3>
                <p class="govuk-body">Government services may set additional cookies and, if so, will have their own cookie policy and banner.</p>
                <a class="govuk-link govuk-notification-banner__link cookie-settings__prev-page" href="#" data-module="gem-track-click" data-track-category="cookieSettings" data-track-action="Back to previous page">Go back to the page you were looking at</a>
            </div>
        </div>
    </div>

    <div class="gem-c-title govuk-!-margin-top-8 govuk-!-margin-bottom-8">

        <h1 class="gem-c-title__text govuk-heading-xl">
            Cookies on Energy Entrepreneurs Fund 9
        </h1>
    </div>

    <div class="gem-c-govspeak govuk-govspeak " data-module="govspeak">

        <p>Cookies are files saved on your phone, tablet or computer when you visit a website.</p>
        <p>We use cookies to make the Energy Entrepreneurs Fund (EEF) work and collection information about how you use our service.</p>

    </div>

    <h2 class="gem-c-heading gem-c-heading--font-size-27 gem-c-heading--padding govuk-!-margin-bottom-3">Cookie settings</h2>

    <div class="cookie-settings__no-js">

        <div class="gem-c-govspeak govuk-govspeak " data-module="govspeak">

            <p>We use Javascript to set most of our cookies. Unfortunately Javascript is not running on your browser, so you cannot change your settings. You can try:</p>
            <ul>
                <li>reloading the page</li>
                <li>turning on Javascript in your browser</li>
            </ul>

        </div>
    </div>


    <div class="cookie-settings__form-wrapper">
        
        <form data-module="cookie-settings">

            <h2 class="govuk-heading-l govuk-!-margin-top-6">Essential cookies (strictly necessary)</h2>

            <p class="govuk-body">Essential cookies keep your information secure while you use the Energy Entrepreneurs Fund (EEF). We do not need to ask permission to use them.</p>

            <table class="govuk-table">
                <caption class="govuk-table__caption">Essential cookies we use</caption>
                <thead class="govuk-table__head">
                    <tr class="govuk-table__row">
                        <th scope="col" class="govuk-table__header">Name</th>
                        <th scope="col" class="govuk-table__header">Purpose</th>
                        <th scope="col" class="govuk-table__header">Expires</th>
                    </tr>
                </thead>
                <tbody class="govuk-table__body">
                    <tr class="govuk-table__row">
                        <th scope="row" class="govuk-table__header">orchauth_Default</th>
                        <td class="govuk-table__cell">Used to keep user signed in</td>
                        <td class="govuk-table__cell">30 minutes/Session</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th scope="row" class="govuk-table__header">inzfs_registration</th>
                        <td class="govuk-table__cell">Used to allow the user securely answer registration questionnaire</td>
                        <td class="govuk-table__cell">30 minutes/Session</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th scope="row" class="govuk-table__header">Identity.TwoFactorUserId</th>
                        <td class="govuk-table__cell">Used to allow the user securely enter 2FA</td>
                        <td class="govuk-table__cell">30 minutes/Session</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th scope="row" class="govuk-table__header">JSESSIONID</th>
                        <td class="govuk-table__cell">Used to identify the user session when communicating with the server</td>
                        <td class="govuk-table__cell">Session</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th scope="row" class="govuk-table__header">orchantiforgery_Default%2fapp</th>
                        <td class="govuk-table__cell">Used to prevent Cross-Site Request Forgery attacks</td>
                        <td class="govuk-table__cell">Session</td>
                    </tr>
                </tbody>
            </table>



            <h2 class="govuk-heading-l govuk-!-margin-top-6">Analytics cookies (optional)</h2>

            <p class="govuk-body">With your permission, we use Google Analytics to collect data about how you use the Energy Entrepreneurs Fund (EEF). This information helps us to improve our service.</p>

            <p class="govuk-body">Google is not allowed to use or share our analytics data with anyone.</p>

            <p class="govuk-body">Google Analytics stores anonymised information about:</p>

            <ul class="govuk-list govuk-list--bullet">
                <li>how you got to the Energy Entrepreneurs Fund (EEF) </li>
                <li>the pages you visit on the Energy Entrepreneurs Fund (EEF)  and how long you spend on them</li>
                <li>any errors you see while using the Energy Entrepreneurs Fund (EEF)</li>
            </ul>

            <table class="govuk-table">
                <caption class="govuk-table__caption">Analytics cookies we use</caption>
                <thead class="govuk-table__head">
                    <tr class="govuk-table__row">
                        <th scope="col" class="govuk-table__header">Name</th>
                        <th scope="col" class="govuk-table__header">Purpose</th>
                        <th scope="col" class="govuk-table__header">Expires</th>
                    </tr>
                </thead>
                <tbody class="govuk-table__body">
                    <tr class="govuk-table__row">
                        <th scope="row" class="govuk-table__header">_ga</th>
                        <td class="govuk-table__cell">Used to distinguish users.</td>
                        <td class="govuk-table__cell">2 years</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th scope="row" class="govuk-table__header">JSESSIONID</th>
                        <td class="govuk-table__cell">Used to persist user session data on our platform.</td>
                        <td class="govuk-table__cell">When you close your browser</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th scope="row" class="govuk-table__header">_ga_&lt;container-id></th>
                        <td class="govuk-table__cell">Used to persist user session state.</td>
                        <td class="govuk-table__cell">2 years</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th scope="row" class="govuk-table__header">_gat_UA-[random number]</th>
                        <td class="govuk-table__cell">Used to reduce the number of requests.</td>
                        <td class="govuk-table__cell">1 minute</td>
                    </tr>
                    <tr class="govuk-table__row">
                        <th scope="row" class="govuk-table__header">__VCAP_ID__</th>
                        <td class="govuk-table__cell">Used to persist user session data on our platform.</td>
                        <td class="govuk-table__cell">When you close your browser</td>
                    </tr>
                </tbody>
            </table>



            <div class="govuk-form-group govuk-!-margin-bottom-6">
                <fieldset class="govuk-fieldset" aria-describedby="hint-b4b76c64">
                    <legend class="govuk-fieldset__legend govuk-fieldset__legend--m">

                        <h2 class="govuk-fieldset__heading">Do you want to accept analytics cookies?</h2>
                    </legend>

                    <div class="govuk-radios">
                        <div class="gem-c-radio govuk-radios__item">
                            <input type="radio" name="cookies-campaigns" id="radio-fa2a5fed-0" value="on" class="govuk-radios__input">

                            <label for="radio-fa2a5fed-0" class="gem-c-label govuk-label govuk-radios__label">Yes</label>


                        </div>

                        <div class="gem-c-radio govuk-radios__item">
                            <input type="radio" name="cookies-campaigns" id="radio-fa2a5fed-1" value="off" class="govuk-radios__input">

                            <label for="radio-fa2a5fed-1" class="gem-c-label govuk-label govuk-radios__label">No</label>


                        </div>

                    </div>
                </fieldset>
            </div>


            <button class="gem-c-button govuk-button" type="submit">Save changes</button>


        </form>
    </div>
