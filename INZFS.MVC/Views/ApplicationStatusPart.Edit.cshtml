
@model ApplicationStatusPart
@using INZFS.MVC.ViewModels
@using INZFS.MVC.Models
@using INZFS.MVC.TagHelpers



<input type="hidden" name="contentType" value="ApplicationStatus" />



div class="govuk-form-group">
<fieldset class="govuk-fieldset" role="group">
    <legend class="govuk-fieldset__legend govuk-fieldset__legend--l">
        What do you think are the barriers to the commercial exploitation of your innovation?
    </legend>
    <div id="waste-hint" class="govuk-hint">
        Select all that apply.
    </div>
    <div class="govuk-checkboxes">
        <div class="govuk-checkboxes__item">
            <input class="govuk-checkboxes__input" asp-for="Approved" type="checkbox" value="true">
            <label class="govuk-label govuk-checkboxes__label" asp-for="Approved">
                Approved
            </label>
        </div>
        <div class="govuk-checkboxes__item">
            <input class="govuk-checkboxes__input" asp-for="Rejected" type="checkbox" value="true">
            <label class="govuk-label govuk-checkboxes__label" asp-for="Rejected">
                Rejected
            </label>
        </div>
    </div>

</fieldset>
</div>
