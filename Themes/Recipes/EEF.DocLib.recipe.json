    {
      "name": "Templates",
      "Templates": {
        "Content__EEFDocLibPage": {
          "Content": "<main class=\"govuk-main-wrapper \" id=\"main-content\" role=\"main\">\r\n\r\n    <div class=\"govuk-grid-row\">\r\n      <div class=\"govuk-grid-column-two-thirds\">\r\n        <h1 class=\"govuk-heading-xl\">{{ Model.ContentItem.Content.TitlePart.Title }}</h1>\r\n      </div>\r\n    </div>\r\n\r\n\r\n  <div class=\"govuk-grid-row\">\r\n    \r\n    <div class=\"govuk-grid-column-two-thirds\">\r\n      \r\n      {{ Model.ContentItem.Content.EEFDocLibPage.Description.Html | liquid }}\r\n\r\n      {% for doc_section in Model.ContentItem.Content.DocumentSection.ContentItems %}\r\n      <h2 class=\"govuk-heading-m\">{{ doc_section.DisplayText }}</h2>\r\n      <p class=\"govuk-body-s\">{{ doc_section.EEFDocSection.Description.Text }}</p>\r\n\r\n      {% for section_doc in doc_section.EEFDocSection.EEFDocumentLibraryPickerField.Paths %}\r\n      <div class=\"govuk-grid-row\">\r\n        <div class=\"govuk-grid-column-one-quarter govuk-!-padding-left-0\">\r\n          <img src=\"assets/images/doc.PNG\" alt=\"Document download\">\r\n        </div>\r\n        <div class=\"govuk-grid-column-three-quarters govuk-!-margin-top-3\">\r\n          <p class=\"govuk-body-l govuk-!-margin-bottom-1\"><a class=\"govuk-link\" href=\"{{ section_doc }}\">{{ section_doc | split: \"/\" | last }}</a></p>\r\n          <p class=\"govuk-body govuk-!-margin-bottom-1 govuk-colour--mid-grey\">{{ section_doc | split: \"/\" | last | split: \".\" | last | upcase }}, 13.2K</p>\r\n          <p class=\"govuk-body govuk-!-margin-bottom-1\">This file may not be suitable for users of assistive technology.</p>\r\n\r\n          <details class=\"govuk-details govuk-!-margin-bottom-6\" data-module=\"govuk-details\">\r\n            <summary class=\"govuk-details__summary\">\r\n              <span class=\"govuk-details__summary-text\">\r\n                Request an accessible format\r\n              </span>\r\n            </summary>\r\n            <div class=\"govuk-details__text\">\r\n              Lorem Ipsum Lorem Ipsum Lorem Ipsum Lorem Ipsum Lorem Ipsum Lorem Ipsum Lorem Ipsum Lorem Ipsum Lorem Ipsum \r\n            </div>\r\n          </details>\r\n\r\n        </div>\r\n      </div>\r\n\t  {% endfor %}\r\n\r\n\t  {% endfor %}\r\n\r\n    </div>\r\n\r\n    <div class=\"govuk-grid-column-one-third\">\r\n\r\n      <!-- The Related items component is not part of GOV.UK Frontend but will be styled if used in the Prototype Kit -->\r\n\r\n      <aside class=\"app-related-items\" role=\"complementary\">\r\n\r\n        <p class=\"govuk-body-m govuk-colour--mid-grey govuk-!-margin-bottom-0\">Deadline</p>\r\n        <p class=\"govuk-body-m govuk-!-margin-bottom-10\">25 March at 11.59pm</p>\r\n\r\n        <p class=\"govuk-body-m govuk-colour--mid-grey govuk-!-margin-bottom-0\">Application</p>\r\n        <p class=\"govuk-body-m govuk-!-margin-bottom-10\">EEF18453SA</p>\r\n\r\n      </aside>\r\n\r\n    </div>\r\n  </div>\r\n  </main>",
          "Description": "Liquid Template for EEF Document Library"
        }
      }
