        {
            "name": "Templates",
            "Templates": {
                "Content__EEFDocLibPage": {
                    "Content": "<div class=\"govuk-grid-row\" style=\"position: relative;transform: translateY(-50%);\">\r\n    <div class=\"govuk-grid-column-three-quarters\"> \r\n      <nav aria-label=\"breadcrumb\" class=\"govuk-breadcrumbs\">\r\n        <ol class=\"govuk-breadcrumbs__list\">\r\n          <li class=\"govuk-breadcrumbs__list-item\">\r\n            <a class=\"govuk-breadcrumbs__link\" href=\"/FundApplication/section/application-overview\">Apply for the Energy Entrepreneurs Fund: phase 9</a>\r\n          </li>\r\n          <li class=\"govuk-breadcrumbs__list-item\" aria-current=\"page\">\r\n           Download documents\r\n          </li>\r\n        </ol>\r\n      </nav>\r\n    </div>\r\n    <div class=\"govuk-grid-column-one-quarter govuk-youraccount-right\">  \r\n      <p><a class=\"govuk-body-m govuk-link\" href=\"/admin\">Your account</a></p>\r\n    </div>\r\n  </div>\r\n<main class=\"govuk-main-wrapper \" id=\"main-content\" role=\"main\">\r\n\r\n    <div class=\"govuk-grid-row\">\r\n      <div class=\"govuk-grid-column-two-thirds\">\r\n        <h1 class=\"govuk-heading-xl\">{{ Model.ContentItem.Content.TitlePart.Title }}</h1>\r\n      </div>\r\n    </div>\r\n\r\n  <div class=\"govuk-grid-row\">\r\n    \r\n    <div class=\"govuk-grid-column-two-thirds\">\r\n      \r\n      {{ Model.ContentItem.Content.EEFDocLibPage.Description.Html | liquid }}\r\n\r\n      {% for doc_section in Model.ContentItem.Content.DocumentSection.ContentItems %}\r\n      <h2 class=\"govuk-heading-m\">{{ doc_section.DisplayText }}</h2>\r\n      <p class=\"govuk-body-s\">{{ doc_section.EEFDocSection.Description.Text }}</p>\t  \r\n\r\n      {% for eef_doc in doc_section.BagPart.ContentItems %}\r\n      <div class=\"govuk-grid-row\">\r\n        <div class=\"govuk-grid-column-one-quarter govuk-!-padding-left-0\">\r\n          <img src=\"/media/{{ eef_doc.EEFGuidanceDocument.FileTypeIcon.Paths }}\" alt=\"Document download\">\r\n        </div>\r\n        <div class=\"govuk-grid-column-three-quarters govuk-!-margin-top-3\">\r\n          <p class=\"govuk-body-l govuk-!-margin-bottom-1\"><a class=\"govuk-link\" href=\"/media/{{ eef_doc.EEFGuidanceDocument.MainDocument.Paths[0] }}\">{{ eef_doc.TitlePart.Title }}</a></p>\r\n          <p class=\"govuk-body govuk-!-margin-bottom-1 govuk-colour--mid-grey\">{{ eef_doc.EEFGuidanceDocument.SubTitle.Text }}</p>\r\n          <p class=\"govuk-body govuk-!-margin-bottom-6\">{{ eef_doc.EEFGuidanceDocument.Description.Html | liquid }}</p>\r\n          {% if eef_doc.EEFGuidanceDocument.AccessibleDocument.Paths[0] %}\r\n          <p class=\"govuk-body-m\"><a class=\"govuk-link\" href=\"/media/{{ eef_doc.EEFGuidanceDocument.AccessibleDocument.Paths[0] }}\">Download an accessible text version</a></p>\r\n\t\t  {% endif %}\r\n        </div>\r\n      </div>\r\n\t  {% endfor %}\r\n\r\n\t  {% endfor %}\r\n\r\n    </div>\r\n\r\n    <div class=\"govuk-grid-column-one-third\">\r\n\r\n      <!-- The Related items component is not part of GOV.UK Frontend but will be styled if used in the Prototype Kit -->\r\n\r\n      <aside class=\"app-related-items\" role=\"complementary\">\r\n\r\n        <h3 class=\"govuk-heading-s\" id=\"subsection-title\">Related content</h3>\r\n        <p class=\"govuk-body-m govuk-!-margin-bottom-3\"><a class=\"govuk-body-m govuk-!-margin-bottom-10\" href=\"/eligibilityInfo/Eligibility-Start\">Check if EEF is the right fund for you</a></p>\r\n<p class=\"govuk-body-m govuk-!-margin-bottom-3\"><a class=\"govuk-link\" href=\"https://www.gov.uk/business-finance-support\">Find finance and support for your business</a></p>\r\n \r\n\r\n        \r\n\r\n      </aside>\r\n\r\n    </div>\r\n  </div>\r\n  </main>",
                    "Description": "Liquid Template for EEF Document Library"
                }
            }
        },
